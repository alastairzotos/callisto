{
	"include": ["src"],
	"compilerOptions": {
		"strict": true,
    "moduleResolution": "node",
		"target": "es5",
		"noImplicitAny": false,
		"outDir": "dist",
		"sourceMap": false,
		"declaration": true,
		"experimentalDecorators": false,
		"emitDecoratorMetadata": false,
		"baseUrl": "src",
		"skipLibCheck": true
	}
}
